{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server={DB_HOST};Port={DB_PORT};Database={DB_NAME};User={DB_USER};Password={DB_PASSWORD};"
  },
  "Jwt": {
    "Key": "YourSecretKeyHere"
  }
}
